<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ticket_bookingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".shaw"></activity>
        <activity android:name=".ticketbooking" />
        <activity android:name=".fast10" />
        <activity android:name=".batmandark" />
        <activity android:name=".ticket_profilepage" />
        <activity android:name=".ticket_paymentpage" />
        <activity android:name=".ticket_bookingpage" />
        <activity android:name=".ticket_homepage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>